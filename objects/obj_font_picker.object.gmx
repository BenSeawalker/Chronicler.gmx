            <string>///find fonts
fonts[|0] = FS_file_find_first("%windir%\Fonts\*.ttf",0);

var i = 0;
var fontdir = environment_get_variable("windir")+"\Fonts\";
var targetdir = environment_get_variable("localappdata")+"\Chronicler\Fonts\";
while(fonts[|i] != "")
{
    
    if(!FS_file_exists(targetdir+fonts[|i]))
    {
        show_debug_message(fontdir+fonts[|i]);
        show_debug_message(targetdir+fonts[|i]);
        show_debug_message("");
        
        FS_directory_create(targetdir);
        FS_file_copy(fontdir+fonts[|i],targetdir+fonts[|i]);
    }
    fonts[|++i] = FS_file_find_next();
}

FS_file_find_close();
</string>
