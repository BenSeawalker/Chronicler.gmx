            <kind>1</kind>
            <string>if(mouse_wheel_down())
{
    index = max(index-1,0);
    
    var fname = "Fonts\"+fonts[|index];//string_copy(fonts[|index],1,string_length(fonts[|index])-4);
    show_debug_message(fname);
    
    if(current_font != fnt_main)
        font_delete(current_font);
    current_font = font_add(fname,13,false,false,32,128);
    draw_set_font(current_font);
}

if(mouse_wheel_up())
{
    index = min(index+1,ds_list_size(fonts)-780/20);
    
    var fname = "Fonts\"+fonts[|index];//string_copy(fonts[|index],1,string_length(fonts[|index])-4);
    show_debug_message(fname);
    
    if(current_font != fnt_main)
        font_delete(current_font);
    current_font = font_add(fname,13,false,false,32,128);
    draw_set_font(current_font);
}

if(keyboard_check_pressed(vk_escape))
    instance_destroy();
</string>
