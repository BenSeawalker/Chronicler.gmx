            <kind>1</kind>
            <string>///input_old
hover = in_rect(window_mouse_get_x(),window_mouse_get_y(),x,y,width,height);
if(mouse_check_button_pressed(mb_left))
{
    if(hover)
    {
        clicks++;
        
        if(clicks&lt;2){
            change_scene(id);
            wy = window_mouse_get_y();
        }
        else if(editable)
            title.read_only = false;
    }
}

if(mouse_check_button(mb_left))
{
    drag = (selected &amp;&amp; editable &amp;&amp; hover &amp;&amp; title.read_only &amp;&amp; abs(window_mouse_get_y()-wy)&gt;2);
    if(drag)
    {
        var index = max(floor((window_mouse_get_y()-scene_list.y)/minheight),2);
        var s = scene_list.scenes;
        ds_list_delete(s,ds_list_find_index(s,id));
        ds_list_insert(s,index,id);
        
    }
}

if(mouse_check_button_released(mb_left))
{
    drag=false;
}



if(mouse_check_button_pressed(mb_right))
{
    if(hover &amp;&amp; editable)
    {
        var cur_s = current_scene;
        change_scene(id);
        if(show_question("Are you sure you wish to delete "+title.text+"?"))
        {
            var index = ds_list_find_index(scene_list.scenes,id);
            with(obj_parent_bubble) instance_destroy();
            
            if(cur_s==id)
                change_scene(scene_list.scenes[| (index-1)]);
            else
                change_scene(cur_s);
            
            for(var i=0;i&lt;ds_list_size(tempvars);i++)
            {
                with(tempvars[| i]) instance_destroy();
            }
            ds_list_destroy(tempvars);
            ds_list_destroy(bubbles);
            
            ds_list_delete(scene_list.scenes,index);
            instance_destroy();
        }
    }
}


if(!title.read_only &amp;&amp; textbox_focus == title)
{
    if(keyboard_check(vk_anykey))
    {   
        path = "";
    }
}

